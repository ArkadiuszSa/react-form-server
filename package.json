{
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./lib/server.ts",
    "watch": "SET NODE_ENV=dev&& nodemon --exec ts-node -- ./lib/server.ts",
    "prod": "nodemon ./dist/lib/server.js",
    "start": "yarn run build && yarn run prod",
    "u-test": "SET NODE_ENV=unit-test && mocha --timeout 20000 -r ts-node/register ./tests/unit/*",
    "i-test": "SET NODE_ENV=integration-test && mocha -r ts-node/register ./tests/integration/*"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "all": true
  },
  "name": "react-form-server",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "global": "^4.3.2",
    "jsonwebtoken": "^8.3.0",
    "mockgoose": "^7.3.5",
    "mongoose": "^5.2.8",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "typings": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.5",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.2.5",
    "@types/mongoose-promise": "^4.5.7",
    "chai": "^4.0.6",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "mocha": "^5.2.0"
  }
}