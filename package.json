    "express-validator": "^5.3.0",
    "global": "^4.3.2",
    "mongoose": "^5.2.8",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "typings": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
